<Application x:Class="WPF_demo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:WPF_demo.ViewModel"
             xmlns:views="clr-namespace:WPF_demo.View.UserControls">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:NewProbandViewModel}">
            <views:NewProband />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:BmiChartViewModel}">
            <views:BmiChart />
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:LandingPageViewModel}">
            <views:LandingPage />
        </DataTemplate>
    </Application.Resources>
</Application>